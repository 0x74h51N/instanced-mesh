{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": false,
    "noImplicitOverride": true,
    "strictNullChecks": false,
    "stripInternal": true,
    "outDir": "public/examples",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "skipLibCheck": true,
  },
  "include": [
    "src/examples",
    "node_modules/@three.ez"
  ],
}